<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Page Not Found - Code It</title>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css"
      integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />

    <link
      rel="shortcut icon"
      href="/search-code-svgrepo-com.svg"
      type="image/x-icon"
    />

    <link rel="stylesheet" href="/styles.css" />
  </head>
  <body>
    <!-- <div class="error-div"> -->
    <section class="error-div">
      <h1 class="error-title">Page Not Found</h1>
      <p class="error-message">Sorry, the page you are looking for does not exist.</p>
      <a href="/" class="error-link">Go back to home page</a>
    </section>
    <!-- </div> -->
    <script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.12"></script>

    <script src="https://cdn.jsdelivr.net/npm/gsap"></script>

    <script>
      gsap.fromTo(
        ".error-title",
        { x: -800, rotation: 40 },
        { x: 0, rotation: 0, duration: 2, ease: "expo.out" }
      );
      gsap.fromTo(
        ".error-message",
        { x: 2000, rotation: -60 },
        { x: 0, rotation: 0, duration: 1, delay: 0.3, ease: "expo.out" }
      );
      gsap.fromTo(
        ".error-link",
        { y: -800 },
        { y: 0, duration: 1, delay: 0.8, ease: "expo.out" }
      );
